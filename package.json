{
  "name": "@truestamp/canonify",
  "type": "module",
  "version": "1.0.2",
  "description": "A tiny JSON canonicalize function written in Typescript. Creates crypto safe predictable canonicalization of JSON as defined by RFC8785 and is usable in Deno, Node.js, and browsers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/truestamp/truestamp-canonify.git"
  },
  "bugs": {
    "url": "https://github.com/truestamp/truestamp-canonify/issues"
  },
  "homepage": "https://github.com/truestamp/truestamp-canonify",
  "keywords": [
    "Truestamp",
    "JavaScript",
    "TypeScript",
    "JSON",
    "Cryptography",
    "Canonicalization",
    "Canonicalize",
    "Canonical",
    "RFC8785",
    "Signature",
    "Universal",
    "ES Modules",
    "UMD",
    "CommonJS",
    "Deno",
    "Node.js",
    "Browser"
  ],
  "source": "mod.ts",
  "main": "dist/canonify.cjs",
  "umd:main": "dist/canonify.umd.js",
  "module": "./dist/canonify.module.js",
  "unpkg": "./dist/canonify.umd.js",
  "jsdelivr": "./dist/canonify.umd.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    "require": "./dist/canonify.cjs",
    "import": "./dist/canonify.module.js",
    "node": "./dist/canonify.cjs",
    "default": "./dist/canonify.module.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "docs": "npx typedoc --plugin typedoc-plugin-extras --options typedoc.json mod.ts",
    "docs-preview": "npx live-server docs",
    "package-check": "npx package-check",
    "build:web": "microbundle --target=web --tsconfig tsconfig.json --format es,umd,modern",
    "build:node": "microbundle --target=node --tsconfig tsconfig.json --format cjs",
    "build": "npm run package-check && npm run clean && npm run build:web && npm run build:node",
    "dev": "microbundle watch",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "watch": "jest --watchAll",
    "serve": "npx serve --cors -p 8080"
  },
  "author": "Truestamp <support@truestamp.com> (https://www.truestamp.com)",
  "contributors": [
    {
      "name": "Truestamp Inc.",
      "url": "https://www.truestamp.com"
    },
    {
      "name": "cyberphone",
      "url": "https://github.com/cyberphone/json-canonicalization"
    },
    {
      "name": "erdtman",
      "url": "https://github.com/erdtman/canonicalize"
    }
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@compodoc/live-server": "^1.2.3",
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "^27.4.1",
    "@types/jsonfile": "^6.1.0",
    "big.js": "^6.1.1",
    "bignumber.js": "^9.0.0",
    "bn.js": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jsonfile": "^6.1.0",
    "microbundle": "^0.15.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "to-hex": "^0.0.18",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "^4.6.4"
  }
}
